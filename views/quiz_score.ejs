<!DOCTYPE html>
<html lang="en">

<head>
<link rel="stylesheet" href="/styles/body.css" type="text/css" />
</head>

<body class="body-box">
  <div class="body-wrap">
    <!-- Top nav bar (fixed) -->
    <header class="site-header">
      <div class="header-container">

        <div class="header-logo">
          <h3>Quizo</h3>
        </div>

        <div class="header-buttons">
          <ul class="header-links">
            <div>
              <a class="button">Home</a>
            </div>
            <div>
              <a class="button">Logout</a>
            </div>
          </ul>
        </div>
      </div>
    </header>

<!--  Insert from here   -->
    <div class="profile">
      <div class="my-quizzes-title">
      <p>My results</p>
      </div>
      <div class="my-quizzes-box">

        <div class="quizzes-display">

<!--Quiz results go here-->
          <table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Quiz</th>
      <th scope="col">Score</th>
    </tr>
  </thead>
  <tbody>
    <% for (let result in results) { %>
    <tr>
    <td><%= result %></td>
    <td class="score"><%= results[result].score%></td>
    </tr>
    <% }%>
  </tbody>
</table>
        </div>
      </div>
    </div>
  </div>
</body>



<footer>
  <%- include('./partials/footer') %>
</footer>
