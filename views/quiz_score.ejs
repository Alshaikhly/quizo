<!DOCTYPE html>
<html lang="en">

<head>
<link rel="stylesheet" href="/styles/quiz-solved.css" type="text/css" />
<link rel="stylesheet" href="/styles/header.css" type="text/css" />
</head>


<header>
  <%- include('./partials/header') %>
</header>
<body class="body-box">
  <div class="body-wrap">
    <!-- Top nav bar (fixed) -->

<!--  Insert from here   -->
    <div class="profile">
      <div class="my-quizzes-title">
        <p>My results</p>
      </div>
        <div class="my-quizzes-box">

          <div class="quizzes-display">

<!--Quiz results go here-->
            <table class="table table-hover">
              <thead>
                <tr>
                  <th scope="col">Quiz</th>
                  <th scope="col">Score</th>
                </tr>
              </thead>
              <tbody>
                <% for (let result in results) { %>
                  <tr>
                    <td><%= result %></td>
                    <td class="score"><%= results[result].score%></td>
                  </tr>
                <% }%>
              </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <footer id="footer">
    <%- include('./partials/footer') %>
  </footer>
</body>
